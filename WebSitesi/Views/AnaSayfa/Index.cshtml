@using WebSitesi.Models.Siniflar
@model List<AnaSayfa>

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="~/web-WebSitesi -Code/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
    <div class="center">
        <div class="avatar">
            <img src="~/web-WebSitesi -Code/WhatsApp Image 2024-01-26 at 18.55.39.jpeg" alt="">
        </div>
        <div class="content">
            @foreach (var x in Model)
            {
                <h1>@x.isim</h1>
                <h2>@x.unvan</h2>
                <p>@x.aciklama</p>
                <h3>@x.iletisim</h3>
            }

        </div>

        @Html.Action("ikonlar", "AnaSayfa")

        <h1>PROJELERİM VE BECERİLERİM</h1>
        <p>
            Bilişim Etiği Ve Güvenliği, Ofis Yazılımları, Web Tasarım, Grafik Ve Animasyon, Logo Tasarımı, Düğün Daveyetesi,
           Kişisel Ve Kurumsal Kartvizit, Photoshop,Fotoğraf Düzeltme, Java, Python, Java Script, Asker Dağıtımı, 
            Bilet Otamasyonu, Öğrenci Yönetim Sistemleri,  E-ticaret sitesi,  Kişisel Web Sitesi, HTMLL, CSS, Mobil Programlama, 
            Android Uygulama Geliştirme, MS SQL, MY SQL, Oracle, OOP, PostgreSQL, Sistem Analizi, Sistem Tasarımı,
            Siber Güvenlik Politikaları, Etik Hacking, Blog Platformları, C#, 
        </p>
        



    </div>
</body>
</html>
